    <solid android:color="#FFFFFF" >
    </solid>

    <stroke
        android:width="0dp"
        android:color="#989898" >
    </stroke>

    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" >
    </padding>

    <gradient
        android:centerColor="#8B8052"
        android:endColor="#8B8052"
        android:startColor="#8B8052"
        android:type="linear" />

    <corners
        android:bottomLeftRadius="11dp"
        android:bottomRightRadius="11dp"
        android:radius="0dp"
        android:topLeftRadius="0dp"
        android:topRightRadius="0dp" >
    </corners>

</shape>